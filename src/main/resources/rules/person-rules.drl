package rules

import example.droolsdemo.model.Person

// 规则1(CheckAdult)：年龄>=18岁则标记为成年
rule "CheckAdult"
    when
        $person : Person( age > 18)
    then
        $person.setAdult(true);
        $person.setAge(18);
        System.out.println($person.getName() + " is an adult!");
end